    "name" : "a4e5bffa",
    "displayName" : "Sales by Territory",
    "query" : "SELECT dim_date.`Date`\n        , dim_territory.*\n        , dim_product.*\n        , fs.OrderAmount\n        , fs.OrderQuantity\n        , fs.CustomerKey\nFROM mp_data_platform.gold.fact_sales fs\n    JOIN mp_data_platform.gold.dim_date ON fs.OrderDate = dim_date.date_key\n    JOIN mp_data_platform.gold.dim_territory ON fs.TerritoryKey = dim_territory.SalesTerritoryKey\n    JOIN mp_data_platform.gold.dim_product ON fs.ProductKey = dim_product.ProductKey"
  }, {
    "name" : "e84b881b",
    "displayName" : "Returns by Territory",
    "query" : "SELECT dim_date.`Date`\n        , dim_territory.*\n        , dim_product.*        \n        , fs.ReturnAmount\n        , fs.ReturnQuantity\nFROM mp_data_platform.gold.fact_returns fs\n    JOIN mp_data_platform.gold.dim_date ON fs.ReturnDate = mp_data_platform.gold.dim_date.date_key\n    JOIN mp_data_platform.gold.dim_territory ON fs.TerritoryKey = dim_territory.SalesTerritoryKey\n    JOIN mp_data_platform.gold.dim_product ON fs.ProductKey = dim_product.ProductKey"
  }, {
    "name" : "910497cb",
    "displayName" : "v_sales_by_productstyle_region",
    "query" : "SELECT * FROM mp_data_platform.gold.v_sales_by_productstyle_region",
    "columns" : [ {
      "displayName" : "Style_Description",
      "description" : "Style_Description",
      "expression" : "  case\n          when ProductStyle = 'U' then 'Universal' \n          when ProductStyle = 'M' then 'Mens'\n          when ProductStyle = 'W' then 'Womens'\n          when ProductStyle = '0' then 'Unknown'\n      end"
    }, {
      "displayName" : "Sales_by_product_styles",
      "description" : "",
      "expression" : "SUM(OrderAmount)  / count(distinct productStyle)"
    } ]
  }, {
    "name" : "8a4f5cd4",
    "displayName" : "productstylekpi1",
    "query" : "/* metric-view-reference */\nSELECT * FROM mp_data_platform.gold.productstylekpi1"
  }, {
    "name" : "133fa4c4",
    "displayName" : "Sales by Territory (Forecasted)",
    "query" : "WITH original_table AS (\n    SELECT \n  DATE_TRUNC(\"MONTH\", `Date`) AS Date,\n  SUM(`OrderQuantity`) AS OrderQuantity\nFROM (\n    SELECT dim_date.`Date`\n        , dim_territory.*\n        , dim_product.*\n        , fs.OrderAmount\n        , fs.OrderQuantity\n        , fs.CustomerKey\nFROM mp_data_platform.gold.fact_sales fs\n    JOIN mp_data_platform.gold.dim_date ON fs.OrderDate = dim_date.date_key\n    JOIN mp_data_platform.gold.dim_territory ON fs.TerritoryKey = dim_territory.SalesTerritoryKey\n    JOIN mp_data_platform.gold.dim_product ON fs.ProductKey = dim_product.ProductKey\n) GROUP BY 1\n  ),\n  dates AS (\n    SELECT \n      MAX(Date) AS max_date_column,\n      MIN(Date) AS min_date_column,\n      MAX_BY(OrderQuantity, Date) AS OrderQuantity\n    FROM original_table\n  ),\n  forecast_table AS (\n    SELECT \n      Date,\n      OrderQuantity_forecast,\n      OrderQuantity_upper,\n      OrderQuantity_lower,\n      NULL AS OrderQuantity\n    FROM AI_FORECAST(\n      TABLE(original_table),\n      horizon => (\n        SELECT\n          max_date_column + MAKE_DT_INTERVAL(CAST(FLOOR(DATEDIFF(max_date_column, min_date_column) * 0.5) AS INT), 0, 0, 0)\n        FROM dates\n      ),\n      time_col => 'Date',\n      value_col => 'OrderQuantity'\n    )\n  )\n\n  SELECT * FROM forecast_table\n  UNION ALL\n  SELECT\n    Date,\n    NULL AS OrderQuantity_forecast,\n    NULL AS OrderQuantity_upper,\n    NULL AS OrderQuantity_lower,\n    OrderQuantity\n  FROM original_table\n    WHERE Date < (SELECT max_date_column FROM dates)\n  UNION ALL\n  SELECT\n    max_date_column AS Date,\n    OrderQuantity AS OrderQuantity_forecast,\n    OrderQuantity AS OrderQuantity_upper,\n    OrderQuantity AS OrderQuantity_lower,\n    OrderQuantity\n  FROM\n    dates"
  }, {
    "name" : "f654cce3",
    "displayName" : "Sales by Territory (Forecasted)",
    "query" : "WITH original_table AS (\n    SELECT \n  DATE_TRUNC(\"MONTH\", `Date`) AS Date,\n  SUM(`OrderQuantity`) AS OrderQuantity\nFROM (\n    SELECT dim_date.`Date`\n        , dim_territory.*\n        , dim_product.*\n        , fs.OrderAmount\n        , fs.OrderQuantity\n        , fs.CustomerKey\nFROM mp_data_platform.gold.fact_sales fs\n    JOIN mp_data_platform.gold.dim_date ON fs.OrderDate = dim_date.date_key\n    JOIN mp_data_platform.gold.dim_territory ON fs.TerritoryKey = dim_territory.SalesTerritoryKey\n    JOIN mp_data_platform.gold.dim_product ON fs.ProductKey = dim_product.ProductKey\n) GROUP BY 1\n  ),\n  dates AS (\n    SELECT \n      MAX(Date) AS max_date_column,\n      MIN(Date) AS min_date_column,\n      MAX_BY(OrderQuantity, Date) AS OrderQuantity\n    FROM original_table\n  ),\n  forecast_table AS (\n    SELECT \n      Date,\n      OrderQuantity_forecast,\n      OrderQuantity_upper,\n      OrderQuantity_lower,\n      NULL AS OrderQuantity\n    FROM AI_FORECAST(\n      TABLE(original_table),\n      horizon => (\n        SELECT\n          max_date_column + MAKE_DT_INTERVAL(CAST(FLOOR(DATEDIFF(max_date_column, min_date_column) * 0.5) AS INT), 0, 0, 0)\n        FROM dates\n      ),\n      time_col => 'Date',\n      value_col => 'OrderQuantity'\n    )\n  )\n\n  SELECT * FROM forecast_table\n  UNION ALL\n  SELECT\n    Date,\n    NULL AS OrderQuantity_forecast,\n    NULL AS OrderQuantity_upper,\n    NULL AS OrderQuantity_lower,\n    OrderQuantity\n  FROM original_table\n    WHERE Date < (SELECT max_date_column FROM dates)\n  UNION ALL\n  SELECT\n    max_date_column AS Date,\n    OrderQuantity AS OrderQuantity_forecast,\n    OrderQuantity AS OrderQuantity_upper,\n    OrderQuantity AS OrderQuantity_lower,\n    OrderQuantity\n  FROM\n    dates"
  }, {
    "name" : "bfdb5bcd",
    "displayName" : "Customers detail",
    "query" : "SELECT  dc.CustomerKey\n        , dc.age\n        , dc.Occupation\n        , dc.HomeOwner\n        , dc.EmailAddress\n        , fs.OrderAmount\n        , fs.OrderQuantity\nFROM mp_data_platform.gold.fact_sales fs\nJOIN mp_data_platform.gold.dim_customer dc\n        ON fs.CustomerKey = dc.CustomerKey"
  } ],
